<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="model">


    <class name="PanelShareNegotiations" table="panel_share_negotiations">

    
        <id name="id" column="id">
            <generator class="native"/>
        </id>
       <property name="shareNegotiationId" type="java.lang.Long" column="share_negotiationId"/>
       <property name="panelId" type="java.lang.Long" column="panelId"></property>
      
    </class>

    


</hibernate-mapping>



