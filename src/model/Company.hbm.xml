<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="model">


    <class name="Company" table="company">

    
        <id name="id" column="id">
            <generator class="native"/>
        </id>

        
       <property name="logoUrl" type="java.lang.String" column="logoUrl"/>
       <property name="name" type="java.lang.String" column="name"/>
       <property name="description" type="java.lang.String" column="description"/>
       <property name="siteUrl" type="java.lang.String" column="siteUrl"/>
       <property name="shareId" type="java.lang.Long" column="shareId"/>
       <property name="userId" type="java.lang.Long" column="userId"/>
       
    </class>
    


</hibernate-mapping>
