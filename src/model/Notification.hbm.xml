<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="model">


    <class name="Notification" table="Notification">

    
        <id name="id" column="id">
            <generator class="native"/>
        </id>
       
       <property name="text" type="java.lang.String" column="text"/>
       <property name="readed" type="java.lang.Boolean" column="readed"></property>
       <property name="userId" type="java.lang.Long" column="userId"/>
       <property name="date" type="java.util.Date" column="date"/>
    </class>
    


</hibernate-mapping>


