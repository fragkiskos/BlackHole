<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="model">


    <class name="Hit" table="Negotiation">

    
        <id name="id" column="id">
            <generator class="native"/>
        </id>
       
       <property name="shareId" type="java.lang.Long" column="shareId"/>
       <property name="userId" type="java.lang.Long" column="userId"/>
       <property name="newPrice" type="java.lang.Double" column="new_price"/>
       <property name="oldPrice" type="java.lang.Double" column="old_price"/>
       <property name="date" type="java.util.Date" column="date"/>
    </class>
    


</hibernate-mapping>


