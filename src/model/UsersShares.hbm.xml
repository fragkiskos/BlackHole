<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="model">


    <class name="UsersShares" table="Users_Shares">

    
        <id name="id" column="id">
            <generator class="native"/>
        </id>
       <property name="userId" type="java.lang.Long" column="userid"/>
       <property name="shareId" type="java.lang.Long" column="shareid"/>
       <property name="sharesNumber" type="java.lang.Integer" column="count"/>
       
       
    </class>
    


</hibernate-mapping>
